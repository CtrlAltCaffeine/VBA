import numpy as np

def generate_exponential_numbers(start, end, Z):
    # Generate Z exponential numbers between start and end
    exponential_numbers = np.logspace(np.log10(start), np.log10(end), Z)
    return exponential_numbers

if __name__ == "__main__":
    try:
        start = float(input("Enter the start value: "))
        end = float(input("Enter the end value: "))
        Z = int(input("Enter the number of values to generate (Z): "))

        if Z < 2:
            print("Please enter a value of Z greater than or equal to 2.")
        else:
            exponential_numbers = generate_exponential_numbers(start, end, Z)
            print("Generated exponential numbers between", start, "and", end, "in ascending order:")
            print(exponential_numbers)

    except ValueError:
        print("Invalid input. Please enter valid numeric values for start, end, and Z.")

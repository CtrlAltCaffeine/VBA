function sidePivot() {
  var sheet = SpreadsheetApp.getActiveSpreadsheet().getActiveSheet();
  
  // Get values from column J (starting from J9)
  var dateColumnJ = sheet.getRange("J9:J").getValues();

  // Get values from column B (starting from B90) and corresponding OI values from column C (starting from C90)
  var dateColumnB = sheet.getRange("B90:B").getValues();
  var valueColumnC = sheet.getRange("C90:C").getValues();

  // Function to parse "MMM YY" format to a Date object
  function parseTextToDate(text) {
    var parts = text.split(' ');
    var month = parts[0];
    var year = parts[1];
    var months = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
    var monthIndex = months.indexOf(month);
    if (monthIndex === -1) {
      throw new Error("Invalid month in date string: " + text);
    }
    return new Date("20" + year, monthIndex, 1);
  }

  // Loop through the text dates in column J
  for (var i = 0; i < dateColumnJ.length; i++) {
    var textDateJ = dateColumnJ[i][0];
    if (!textDateJ) break; // Exit loop if an empty cell is found in column J

    // Convert text "MMM YY" to Date object
    var dateJ = parseTextToDate(textDateJ);
    var formattedDateJ = Utilities.formatDate(dateJ, Session.getScriptTimeZone(), "MMM-yy");

    // Loop through the dates in column B
    for (var j = 0; j < dateColumnB.length; j++) {
      var cellValueB = dateColumnB[j][0].toString();
      if (cellValueB) {
        var year = cellValueB.substring(0, 4);
        var month = cellValueB.substring(4, 6);
        var dateB = new Date(year, month - 1); // Month is 0-indexed
        var formattedDateB = Utilities.formatDate(dateB, Session.getScriptTimeZone(), "MMM-yy");

        // If dates match, copy the corresponding value from column C to column L
        if (formattedDateJ === formattedDateB) {
          var valueC = valueColumnC[j][0];
          sheet.getRange(i + 9, 12).setValue(valueC); // L column is the 12th column
          break;
        }
      }
    }
  }
}

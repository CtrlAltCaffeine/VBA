import pyautogui
import clipboard
import time
import webbrowser


def read_clipboard_content():
    return clipboard.paste()


def paste_to_google_sheets(sheet_url):
    
    webbrowser.open(sheet_url)
    time.sleep(10)  

    
    pyautogui.click(x=200, y=300)  
    time.sleep(2)

    
    pyautogui.hotkey('ctrl', 'v')










paste_to_google_sheets(sheet_url)
